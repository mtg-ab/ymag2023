{
  "name": "mtg-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "Marin",
  "license": "MIT",
  "env": {
    "dev": "mtg-dev-theo",
    "prod": "mtg-dev-theo"
  },
  "scripts": {
    "clean:dist": "rm -rf dist && mkdir dist ",
    "build": "yarn clean:dist && yarn build:css && yarn build:html && yarn build:js",
    "build:js": "webpack",
    "build:css": "gulp buildCss",
    "build:html": "gulp buildhtml",
    "deploy-dev:css": "hs upload --overwrite --portal=$npm_package_env_dev dist/css/main.css boilerplate-mtg-hubspot/css/main.css",
    "deploy-prod:css": "hs upload --overwrite --portal=$npm_package_env_prod dist/css/main.css boilerplate-mtg-hubspot/css/main.css",
    "deploy-dev:js": "hs upload --overwrite --portal=$npm_package_env_dev dist/js/main.js boilerplate-mtg-hubspot/js/main.js",
    "deploy-prod:js": "hs upload --overwrite --portal=$npm_package_env_prod dist/js/main.js boilerplate-mtg-hubspot/js/main.js",
    "watch:css": "gulp watch",
    "watch:js": "webpack --watch",
    "start": "concurrently \"yarn watch:css\" \"yarn watch:js\"",
    "hs:auth": "hs auth personalaccesskey",
    "hs:sync-prod": " hs upload --overwrite --portal=$npm_package_env_prod dist boilerplate-mtg-hubspot",
    "hs:sync-dev": " hs upload --overwrite --portal=$npm_package_env_dev dist boilerplate-mtg-hubspot",
    "hs:module": "yarn plop;",
    "hs:watch-dev": "hs watch --portal=$npm_package_env_dev src/modules boilerplate-mtg-hubspot",
    "hs:watch-prod": "hs watch --portal=$npm_package_env_prod src/modules boilerplate-mtg-hubspot",
    "deploy:prod": "yarn build && yarn hs:sync-prod",
    "deploy:dev": "yarn build && yarn hs:sync-dev"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@hubspot/cms-cli": "^1.0.11",
    "autoprefixer": "9.7.3",
    "babel-loader": "^8.0.6",
    "cssnano": "^4.1.10",
    "del": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-cheerio": "^1.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-exec": "^5.0.0",
    "gulp-ext-replace": "^0.3.0",
    "gulp-footer": "^2.1.0",
    "gulp-hash": "^4.2.2",
    "gulp-header": "^2.0.9",
    "gulp-load-plugins": "^2.0.8",
    "gulp-postcss": "^9.0.1",
    "gulp-purgecss": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sass-glob-import": "^0.1.0",
    "gulp-svgmin": "^4.1.0",
    "gulp-svgstore": "^9.0.0",
    "gulp-util": "^3.0.8",
    "js-yaml": "^3.13.1",
    "node-sass": "^9.0.0",
    "plop": "^2.5.3",
    "postcss": "^8.4.29",
    "postcss-object-fit-images": "^1.1.2",
    "postcss-preset-env": "^9.1.3",
    "prettier": "^1.19.0",
    "pretty-quick": "^2.0.1",
    "require-dir": "^1.2.0",
    "sass": "^1.66.1",
    "standard-version": "^7.0.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-shell-plugin-next": "^2.3.1"
  },
  "dependencies": {
    "concurrently": "^8.2.1",
    "css-loader": "^6.7.3",
    "jquery": "^3.6.1",
    "minimatch": "^9.0.3",
    "style-loader": "^3.3.1",
    "swiper": "^8.3.2",
    "flickity": "^2.2.1",
    "flickity-fade": "^1.0.0",
    "flickity-fullscreen": "^1.1.1",
    "flickity-imagesloaded": "^2.0.0"
  }
}
